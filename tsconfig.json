{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "esnext"], 
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,            
    "forceConsistentCasingInFileNames": true, 
    "strict": true, 
    "skipLibCheck": true                                 
  },
  "include": ["./src/**/*", "./gatsby-node.ts", "./gatsby-config.ts", "./plugins/**/*"],
  "exclude": ["./node_modules/**/*", "./destiny-timeline-deveditor/**/*"]
}
