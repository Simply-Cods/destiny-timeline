{
    "version": "0.2",
    "language": "en",
    "dictionaries": [
        "workspace",
        "workspace-destiny"
    ],
    "dictionaryDefinitions": [
        {
            "name": "workspace",
            "path": "./config/cspell/workspace-dict.txt",
            "description": "Workspace Dictionary",
            "addWords": true,
            "scope": "workspace"
        },
        {
            "name": "workspace-destiny",
            "path": "./config/cspell/destiny-dict.txt",
            "description": "Workspace Dictionary - Destiny",
            "addWords": true,
            "scope": "workspace"
        }
    ],
    "allowCompoundWords": true,
    "useGitignore": true,
    "ignorePaths": [
        "cspell.json",
        "gatsby-config.ts",
        "package.json",
        ".gitignore",
        ".prettierignore",
        "tsconfig.json",
        "typed-scss-modules.config.ts",
        "yarn.lock",
        ".vscode/extensions.json",
        "/src/images",
        ".husky"
    ]
}